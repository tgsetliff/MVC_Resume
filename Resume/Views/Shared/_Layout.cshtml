<!DOCTYPE html>
<html>
<head>

    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Tom Setliff</title>

    <meta name="description" content="Timeline Style CV / Resume / vCard Bootstrap Template.">
    <meta name="author" content="The Develovers">
    <!-- CSS -->
    <link href="/css/main.css" media="screen, projection" rel="stylesheet" type="text/css">
    <!-- Fav and touch icons -->
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="/ico/apple-touch-icon-144-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="114x114" href="/ico/apple-touch-icon-114-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="72x72" href="/ico/apple-touch-icon-72-precomposed.png">
    <link rel="apple-touch-icon-precomposed" href="/ico/apple-touch-icon-57-precomposed.png">
    <link rel="shortcut icon" href="/ico/faviconheadshot.png">
    @Styles.Render("~/Content/css")
    <script src="/Scripts/jquery-2.1.3.min.js"></script>
    @Scripts.Render("~/bundles/modernizr")

</head>
<body>
    <div class="wrapper">

         <div>
            @RenderBody()
        </div>

    </div>

    <!-- Javascript -->
    <script src="/Scripts/bootstrap.min.js"></script>
    <script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=true"></script>
    <script type="text/javascript" src="/js/jquery.ui.map.full.min.js?ver=3.0"></script>
    <script type="text/javascript" src="/js/jquery.scrollTo-1.4.3.1-min.js"></script>
    <script type="text/javascript" src="/js/jquery.localscroll-1.2.7-min.js"></script>
    <script type="text/javascript" src="/js/main.js"></script>
    @RenderSection("scripts", required: false)
</body>
</html>
