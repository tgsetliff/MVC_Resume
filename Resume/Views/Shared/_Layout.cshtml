<!DOCTYPE html>
<html>
<head>

    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Tom Setliff</title>

    <meta name="description" content="Timeline Style CV / Resume / vCard Bootstrap Template.">
    <meta name="author" content="The Develovers">
    <!-- CSS -->
    <link href="/css/main.css" media="screen, projection" rel="stylesheet" type="text/css">
    <!-- Fav and touch icons -->
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="/ico/apple-touch-icon-144-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="114x114" href="/ico/apple-touch-icon-114-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="72x72" href="/ico/apple-touch-icon-72-precomposed.png">
    <link rel="apple-touch-icon-precomposed" href="/ico/apple-touch-icon-57-precomposed.png">
    <link rel="shortcut icon" href="/ico/faviconheadshot.png">
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

</head>
<body>
    <div class="wrapper">

        <!-- main navigation -->
        <nav class="navbar navbar-default navbar-fixed-top" id="main-nav" role="navigation">
            <div class="container">
                <!-- brand and toggle -->
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".main-nav-collapse">
                        <span class="sr-only">Toggle Navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                </div>
                <!-- navigation links -->
                <div class="collapse navbar-collapse main-nav-collapse">
                    <ul class="nav navbar-nav">
                        <li><a href="~/Views/Profile/Index.cshtml">ABOUT ME</a></li>
                        <li><a href="#skills">SKILLS</a></li>
                        <li><a href="#projects">PROJECTS</a></li>
                        <li><a href="#resume">RESUME</a></li>
                        <li><a href="#contacts">CONTACTS</a></li>
                    </ul>
                </div>
            </div>
        </nav>
        <!-- end main navigation -->
        <!-- header -->
        <header class="jumbotron header" id="header">
            <div class="container">
                <div class="row">
                    <div class="col-md-6 left">
                        <div class="avatar">
                            <img src="/img/headshot.png" class="img-responsive" alt="Profile Picture" />
                        </div>
                    </div>
                    <div class="col-md-6 right">
                        <h1 class="hide">Tom Setliff</h1>
                        <h2 class="heading">HELLO, I'M TOM</h2>
                        <p>Applications Developer from North Carolina. Want to know more about me?</p>
                        <a href="/Profile" class="btn btn-default btn-lg">READ MORE</a><a href="TomSetliffResume.pdf" class="btn btn-primary btn-lg">VIEW/SAVE RESUME</a>
                    </div>
                </div>
            </div>
        </header>

        <div>
            @RenderBody()
        </div>

    </div>
    <!-- END WRAPPER -->
    <!-- Javascript -->
    <script type="text/javascript" src="/js/jquery-1.7.2.min.js"></script>
    <script type="text/javascript" src="/js/bootstrap.min.js"></script>
    <script type="text/javascript" src="/js/modernizr.js"></script>
    <script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=true"></script>
    <script type="text/javascript" src="/js/jquery.ui.map.full.min.js?ver=3.0"></script>
    <script type="text/javascript" src="/js/jquery.scrollTo-1.4.3.1-min.js"></script>
    <script type="text/javascript" src="/js/jquery.localscroll-1.2.7-min.js"></script>
    <script type="text/javascript" src="/js/main.js"></script>
    @RenderSection("scripts", required: false)
</body>
</html>
